        <name>GetUserInfo</name>
        <script><![CDATA[var GetUserInfo = Class.create();
GetUserInfo.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
    getUserDetails: function() {
        // Obtenha o sys_id do usuário logado
        var userSysId = gs.getUserID();
        
        // Query na tabela sys_user para obter os dados do usuário
        var userGR = new GlideRecord('sys_user');
        userGR.get(userSysId);
        
        if (userGR) {
            // Retorne o nome e email do usuário logado
            return JSON.stringify({
                name: userGR.getValue('name'),
                email: userGR.getValue('email')
            });
        }
        return null;
    },

    type: 'GetUserInfo'
});
]]></script>
