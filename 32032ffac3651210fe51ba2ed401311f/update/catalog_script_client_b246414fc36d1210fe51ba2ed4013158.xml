        <script><![CDATA[function onLoad() {
    // Cria uma nova instância de GlideAjax para chamar o Script Include
    var ga = new GlideAjax('GetUserInfo');
    ga.addParam('sysparm_name', 'getUserDetails');

    // Corrige a chamada com uma função de callback
    ga.getXMLAnswer(function(response) {
        var result = response;
        if (result) {
            var userInfo = JSON.parse(result);
            
            // Exemplo: preencher os campos com o nome e email do usuário logado
            g_form.setValue('full_name', userInfo.name);
            g_form.setValue('email', userInfo.email);
        }
    });
}
]]></script>
