        <name>GetEventSeats</name>
        <script><![CDATA[var GetEventSeats = Class.create();
GetEventSeats.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
getAvailableSeats: function() {
    var eventId = this.getParameter('sysparm_event_id');

    // Consulta a tabela de eventos usando o sys_id do evento desejado
    var eventGR = new GlideRecord('x_1444656_event_0_event');
    if (eventGR.get(eventId)) {
        return JSON.stringify({
            available_seats: eventGR.getValue('avaliable_seats')
        });
    }
    return null;
},

type: 'GetEventSeats'
});]]></script>
